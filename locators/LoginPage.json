{
  "url": "https://laboratorio.qaminds.com/index.php?route=account/login",
  "locators": {
    "email_input": {
      "by": "ID",
      "value": "input-email"
    },
    "password_input": {
      "by": "ID",
      "value": "input-password"
    },
    "login_btn": {
      "by": "XPATH",
      "value": "//input[@value=\"Login\"]"
    },
    "warning_message": {
      "by": "CLASS_NAME",
      "value": "alert-danger"
    },
    "forgot_link":{
      "by": "LINK_TEXT",
      "value": "Forgotten Password"
    },
    "continue_button": {
      "by": "XPATH",
      "value": "//input[@value=\"Continue\"]"
    },
    "alert_success": {
      "by": "XPATH",
      "value": "//*[@class=\"alert alert-success alert-dismissible\"]"
    }
  }
}
